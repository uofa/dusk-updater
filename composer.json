{
    "name": "uofa/laravel-dusk-updater",
    "description": "Updater for Laravel Dusk ChromeDriver binaries",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "symfony/process": "^3.4|^4.2|^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.22|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\DuskUpdater\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\DuskUpdater\\DuskServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
